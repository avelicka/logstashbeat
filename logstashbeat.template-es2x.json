{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "dynamic_templates": [
        {
          "fields": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string",
            "path_match": "fields.*"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "events": {
          "properties": {
            "filter": {
              "type": "long"
            },
            "in": {
              "type": "long"
            },
            "out": {
              "type": "long"
            }
          }
        },
        "jvm": {
          "properties": {
            "threads": {
              "properties": {
                "count": {
                  "type": "long"
                },
                "peak_count": {
                  "type": "long"
                }
              }
            }
          }
        },
        "process": {
          "properties": {
            "cpu": {
              "properties": {
                "percent": {
                  "type": "long"
                },
                "total_in_millis": {
                  "type": "long"
                }
              }
            },
            "max_file_descriptors": {
              "type": "long"
            },
            "mem": {
              "properties": {
                "total_virtual_in_bytes": {
                  "type": "float"
                }
              }
            },
            "open_file_descriptors": {
              "type": "long"
            },
            "peak_open_file_descriptors": {
              "type": "long"
            }
          }
        },
        "tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "logstashbeat-*"
}